{
 "env": {
     "browser": true,
     "es6": true
 },
 "extends": [
     "plugin:vue/recommended"
 ],
 "globals": {
     "Atomics": "readonly",
     "SharedArrayBuffer": "readonly"
 },
 "parserOptions": {
     "ecmaVersion": 2018,
     "sourceType": "module"
 },
 "plugins": [
     "vue"
 ],
 "rules": {
   "indent": ["error", 2],
   "linebreak-style": 0,
   "import/prefer-default-export": "off",
   "vue/max-attributes-per-line": ["error", {
   "singleline": 3,
   "multiline": {
     "max": 3,
     "allowFirstLine": false
   }
 }]
 }
}
